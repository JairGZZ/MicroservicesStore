{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Product/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5220
        }
      ],
      "UpstreamPathTemplate": "/gateway/product/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "product"
    },
    {
      "DownstreamPathTemplate": "/api/Category/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5220
        }
      ],
      "UpstreamPathTemplate": "/gateway/category/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "category"
    }
  ],

  "SwaggerEndPoints": [
    {
      "Key": "product",
      "Config": [
        {
          "Name": "Product API",
          "Version": "v1",
          "Url": "http://localhost:5220/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "category",
      "Config": [
        {
          "Name": "Category API",
          "Version": "v1",
          "Url": "http://localhost:5220/swagger/v1/swagger.json"
        }
      ]
    }
  ],

  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5089"
  }
}
